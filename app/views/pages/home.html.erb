<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>

<ul>
  <% @users.each do |user| %>
    <strong><%= link_to user.first_name, user_path(user) %></strong>
    <li><%= user.task %></li>
    <li><%= user.hourly_price %></li>
    <%= link_to "Make a booking!", new_user_booking_path(user) %>
  <% end %>
</ul>
